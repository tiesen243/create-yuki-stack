{
  "name": "create-yuki-stack",
  "version": "0.0.97",
  "description": "A CLI tool for scaffolding type-safe, full-stack TypeScript applications with best practices and customizable.",
  "keywords": [
    "boilerplate-generator",
    "cli",
    "create-yuki-stack",
    "full-stack",
    "monorepo",
    "scaffolding",
    "starter",
    "turborepo",
    "type-safe",
    "typescript",
    "workspace"
  ],
  "homepage": "https://create-yuki-stack.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiesen243/create-yuki-stack",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": {
    "name": "Tiesen",
    "url": "https://tiesen.id.vn"
  },
  "type": "module",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "pub:dev": "tsdown && bun link",
    "pub:release": "tsdown && bun publish --tag latest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "../../tooling/prettier/index.js",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.4.1",
    "gradient-string": "^3.0.0",
    "trpc-cli": "^0.9.2"
  },
  "devDependencies": {
    "@types/node": "24.0.1",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.12.7",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}

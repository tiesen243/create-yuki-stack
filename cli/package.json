{
  "name": "create-yuki-stack",
  "version": "0.2.2",
  "description": "A CLI tool for scaffolding type-safe, full-stack TypeScript applications with best practices and customizable.",
  "keywords": [
    "cli",
    "create-yuki-stack",
    "full-stack",
    "scaffolding",
    "turborepo",
    "workspace"
  ],
  "homepage": "https://create-yuki-stack.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiesen243/create-yuki-stack",
    "directory": "cli"
  },
  "license": "MIT",
  "author": {
    "name": "Tiesen",
    "url": "https://tiesen.id.vn"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch ./src",
    "format": "prettier --check . --ignore-path ../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yuki/prettier-config",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@trpc/server": "^11.7.2",
    "gradient-string": "^3.0.0",
    "picocolors": "^1.1.1",
    "trpc-cli": "^0.12.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "^0.17.0-beta.4",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}

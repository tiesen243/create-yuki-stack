{
  "name": "@{{ name }}/tanstack-start",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "{{ pkm }} with-env vinxi build",
    "clean": "git clean -xdf .cache .output .turbo .vinxi node_modules",
    "dev": "{{ pkm }} with-env vinxi dev --port 3002",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "{{ pkm }} with-env vinxi start --port 3002",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@{{ name }}/prettier-config",
  "dependencies": {
    "@{{ name }}/env": "workspace:*",
    "@{{ name }}/ui": "workspace:*",
    "@tanstack/react-router": "^1.120.18",
    "@tanstack/react-start": "^1.120.18",
    "react": "catalog:react",
    "react-dom": "catalog:react"
  },
  "devDependencies": {
    "@{{ name }}/eslint-config": "workspace:*",
    "@{{ name }}/prettier-config": "workspace:*",
    "@{{ name }}/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:tool",
    "jiti": "^2.4.2",
    "prettier": "catalog:tool",
    "tailwindcss": "catalog:tailwind",
    "typescript": "catalog:tool",
    "vinxi": "^0.5.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}

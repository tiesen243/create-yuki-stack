{
  "name": "@{{ name }}/tanstack-start",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "{{ pkm }} run with-env vite build",
    "clean": "git clean -xdf .cache .nitro .output .tanstack .turbo node_modules",
    "dev": "{{ pkm }} run with-env vite dev",
    "start": "{{ pkm }} run with-env node ./.output/server/index.mjs",
    "typecheck": "tsr generate && tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@tanstack/react-query": "catalog:prod",
    "@tanstack/react-router": "^1.143.11",
    "@tanstack/react-start": "^1.143.11",
    "@{{ name }}/lib": "workspace:*",
    "@{{ name }}/ui": "workspace:*",
    "@{{ name }}/validators": "workspace:*",
    "react": "catalog:react",
    "react-dom": "catalog:react"
  },
  "devDependencies": {
    "@tailwindcss/vite": "catalog:tailwind",
    "@tanstack/router-cli": "^1.143.11",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@{{ name }}/tsconfig": "workspace:*",
    "dotenv-cli": "catalog:dev",
    "tailwindcss": "catalog:tailwind",
    "typescript": "catalog:dev",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  }
}

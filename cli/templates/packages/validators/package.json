{
  "name": "@{{ name }}/validators",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./env": "./dist/env.mjs",
    "./env.next": "./dist/env.next.mjs",
    "./env.vite": "./dist/env.vite.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",

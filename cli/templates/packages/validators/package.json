  },
  "dependencies": {
    "@{{ name }}/lib": "workspace:*",
    "zod": "catalog:prod"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "@{{ name }}/tsconfig": "workspace:*",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
